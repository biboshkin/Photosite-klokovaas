{
  "name": "klokovaas_site",
  "version": "1.0.0",
  "description": "Photograther portfolio site",
  "private": true,
  "scripts": {
    "deploy": "npm run clean && npm run build && npm run rsync",
    "rsync": "rsync --rsync-path='/usr/bin/sudo /usr/bin/rsync' -azP ./public/ shurato@luandro.com.com:../../var/www/photography.luandro.com/htdocs",
    "build": "nwb build",
    "clean": "nwb clean",
    "start": "nwb serve",
    "imgs": "svgo -f ./public/images/ && find ./public/images -name '*.jpg' | imageoptim",
    "thumbs": "mkdir thumbs && mogrify -resize 350 -path ./thumbs *.jpg && mogrify -gravity center -crop 275x275+0+0 ./thumbs/*.jpg",
    "test": "nwb test"
  },
  "dependencies": {
    "blazy": "^1.5.2",
    "history": "^1.13.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-imageloader": "^2.1.0",
    "react-images": "^0.2.1",
    "react-masonry-component": "^2.0.0",
    "react-motion": "^0.3.1",
    "react-redux": "^5.0.6",
    "react-router": "^1.0.1",
    "react-rpg": "0.0.2",
    "react-spinkit": "^1.1.4",
    "recompose": "^0.11.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "nwb": "^0.8.0"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
